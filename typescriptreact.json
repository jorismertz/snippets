{
	"Typescript react functional component": {
		"prefix": "rts",
		"body": [
			"import { ReactElement } from 'react';",
			"",
			"interface Props {}",
			"",
			"const $1 = (): ReactElement => {",
			"  return (",
			"    <div>",
			"      <h1>$2</h1>",
			"    </div>",
			"  );",
			"};",
			"",
			"export default $1;"
		]
	 },
	 "Cva component starter": {
		"prefix": "cva",
		"body": [
		"import type { VariantProps } from 'class-variance-authority';",
		"import type { ComponentProps } from 'react';",
		"import { cva } from 'class-variance-authority';",
		"",
		"const style = cva('');",
		"",
		"interface Props extends VariantProps<typeof style>, ComponentProps<'$1'> {}",
		"",
		"export const $2 = ({ children }: Props) => {",
		"  return <$1 className={style()}>{children}</$1>;",
		"};"
		]
	 }
}
